


<%= form_for @vote do |f| %>
  <%= f.hidden_field :experience_id, :value => @experience.id %>
  <%= f.hidden_field :session_browser_platform, :value => browser.platform %>
  <%= f.hidden_field :session_browser_name, :value => browser.name %>
  <%= f.hidden_field :session_browser_version, :value => browser.full_version %>
  <%= f.hidden_field :session_ip, :value => request.remote_ip %>
  <!--clip_id confidence-->

  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>



<div class="btn-group" data-toggle="buttons">
  <% vote_button = 1 %>
  <% @clip_vote.each do |clip| %>
    <label class="btn btn-default">
      <input type="checkbox" checked autocomplete="off">Clip <%= vote_button %>
    </label>
    <% vote_button += 1 %>
  <% end %>
</div>

<h5>Choose your confidence level in this choice</h5>

<div class="btn-group" data-toggle="buttons">
  <% @options.each do |option| %>
    <label class="btn btn-default">
      <input type="checkbox" checked autocomplete="off"><%= option %>
    </label>
  <% end %>
</div>
